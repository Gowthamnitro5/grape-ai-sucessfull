    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/grapeai.png"
    },
    "plugins": ["expo-router", "expo-font"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
